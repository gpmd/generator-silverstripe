{
  "name": "silverstripe/installer",
  "description": "The SilverStripe Framework Installer",
  "require": {
    "php": ">=5.3.2",
    "silverstripe/cms": "<%= silverstripeVersion %>",
    "silverstripe/framework": "<%= silverstripeVersion %>"
  },
  "config": {
    "process-timeout": 600
  },
  "extra": {
    "installer-paths": {
      "./public_html/{$name}": [
        "silverstripe/framework",
        "silverstripe/cms"
      ]
    }
  },
  "minimum-stability": "dev"
}
